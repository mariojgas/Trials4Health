@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Trials4Health</title>

    <environment names="Development">
        <link rel="stylesheet" href="~/css/bootstrap.css" />
        <link rel="stylesheet" href="~/css/site.css" />
        <link rel="stylesheet" href="~/theme/css/animate.css">
        <link rel="stylesheet" href="~/theme/css/linecons.css">
        <link rel="stylesheet" href="~/theme/inc/strip/css/strip.css">
        <link rel="stylesheet" href="~/theme/css/owl.carousel.css">
        <link rel="stylesheet" href="~/theme/style.css">
        <link rel="stylesheet" href="~/theme/css/responsive.css">
        <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
          <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="~/css/bootstrap.min.css">
        <link rel="stylesheet" href="~/theme/css/animate.css">
        <link rel="stylesheet" href="~/theme/css/linecons.css">
        <link rel="stylesheet" href="~/theme/inc/strip/css/strip.css">
        <link rel="stylesheet" href="~/theme/css/owl.carousel.css">
        <link rel="stylesheet" href="~/theme/style.css">
        <link rel="stylesheet" href="~/theme/css/responsive.css">
        <!--[if lt IE 9]>
          <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
          <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->

        <script src="https://code.jquery.com/jquery.min.js"></script>
        <script src="http://cdnjs.cloudflare.com/ajax/libs/waypoints/2.0.3/waypoints.min.js"></script>
        <script src="~/theme/js/jquery.counterup.min.js"></script>
        <script src="~/theme/js/jquery.sticky.js"></script>
        <script src="~/theme/inc/strip/js/strip.pkgd.min.js"></script>
        <script src="~/theme/js/jquery.easing.1.3.min.js"></script>
        <script src="~/theme/js/jquery.tweet.min.js"></script>
        <script src="~/theme/js/owl.carousel.min.js"></script>
        <script src="~/theme/js/isotope.pkgd.min.js"></script>
        <script src="~/theme/js/wow.min.js"></script>
        <!--[if !IE]><!-->
        <script type="text/javascript">new WOW().init();</script>
        <!--<![endif]-->
        <!--[if gte IE 9]>
        <script type="text/javascript">new WOW().init();</script>
        <![endif]-->
        <!--[if lte IE 8]>
            <script type="text/javascript" src="js/ie-opacity-polyfill.js"></script>
        <![endif]-->
        <script src="js/main.js"></script>

        <script asp-fallback-src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js"
                src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery"
                crossorigin="anonymous"
                integrity="sha384-K+ctZQ+LL8q6tP7I94W+qzQsfRV2a+AfHIi9k8z8l9ggpc8X+Ytst4yBo/hH+8Fk">
        </script>
        <script asp-fallback-src="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.7/bootstrap.min.js"
                src="~/lib/bootstrap/dist/js/bootstrap.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
                crossorigin="anonymous"
                integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa">
        </script>
        <script src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>
    @Html.Raw(JavaScriptSnippet.FullScript)
</head>

<body>
    <header class="header">
        <div class="container" style="width:100%">
            <div class="row">
                <div class="col-sm-2">
                    <div class="logo" style="margin-top:-35px;">
                        <h1><a asp-action="Index" asp-controller="Home">Trails4Health</a></h1>

                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                        </div>
                    </div>
                </div>

                <div class="col-sm-9" style="margin-left:60px;margin-top:-35px">

                    <div class="navbar-collapse collapse">
                        <ul class="nav navbar-nav navbar-right text-uppercase">
                            <li class="active" style="a:hover{background-color:white !important; opacity:100 !important;}"><a asp-action="Index" asp-controller="Home">Home</a></li>
                            <li><a asp-action="Estatistica" asp-controller="Home">Estatisticas</a></li>
                            <li><a asp-action="Requisitos" asp-controller="Home">Requisitos</a></li>
                            <li><a asp-action="Equipamentos" asp-controller="Home">Equipamentos</a></li>
                            <li><a asp-action="Cuidados" asp-controller="Home">Cuidados a ter</a></li>
                            <li><a asp-action="PrimeirosSocorros" asp-controller="Home">Primeiros Socorros</a></li>
                            <li><a asp-action="Contactos" asp-controller="Home">Contactos</a></li>
                            <li><a asp-action="Login" asp-controller="Home">Login</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </header> <!-- End header -->

    <section id="slider">

        <div data-velocity="-.2" class="overlay-bg slide-bg" style="background-image:url('/images/imgcar3.jpg')">
            <div class="container" style="margin-top:100px;">
                @RenderBody()
            </div>
        </div>

    </section> <!-- End slider -->


    <footer id="footer" class="section-padding" style="height:170px;padding-top:20px">
        <div class="container">
            <div style="width:100%;">
                <div class="row">
                    <div class="col-md-12" style="margin-top:10px;margin-bottom:-40px">
                        <h2>Links</h2>
                        <table class="table" style="background-color:transparent">
                            <tr>
                                <td><a asp-action="Index" asp-controller="Home">Home</a></td>
                                <td><a asp-action="Estatistica" asp-controller="Home">Estatisticas</a></td>
                                <td><a asp-action="Requisitos" asp-controller="Home">Requisitos</a></td>
                                <td><a asp-action="Equipamentos" asp-controller="Home">Equipamentos</a></td>
                                <td><a asp-action="Cuidados" asp-controller="Home">Cuidados a ter</a></td>
                                <td><a asp-action="PrimeirosSocorros" asp-controller="Home">Primeiros Socorros</a></td>
                                <td><a asp-action="Contactos" asp-controller="Home">Contactos</a></td>
                            </tr>
                        </table>
                    </div>
                </div>


                <div class="row" style="margin-top:-30px;">
                    <div class="col-md-12">
                        <div class="footer-sep"></div>
                    </div>
                    <div class="col-md-5 table" style="margin-top:-30px;">
                        <div class="footer-menu">
                            <ul>
                                <li><a href="">TERMS</a></li>
                                <li><a href="">Privacy Policy</a></li>
                                <li><a asp-action="Contactos" asp-controller="Home">Contactos</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>