@model IEnumerable<Requisitos>
@{
    ViewData["Title"] = "Requisitos";
}

<div class="panel panel-primary" style="margin-top:28px;margin-bottom:10px">
    <div class="panel-heading">
        <h1 class=" panel-title">@ViewData["Title"]</h1>
    </div>


    <div class="panel-body">
        @{


            foreach (Requisitos req in Model)
            {

                <div style="color:white;opacity:1;">
                    <div>
                        Nome: @req.Nome
                        <br />
                        Descrição: @req.Descrição
                    </div>
                </div>
                            <br />
            }
        }
    </div>

</div>
